{
  "name": "alt-vue-grid-layout",
  "version": "2.3.7",
  "description": "A draggable and resizable grid layout, as a Vue component.",
  "keywords": [
    "grid",
    "vuejs",
    "drag",
    "draggable",
    "resize",
    "resizable",
    "fluid",
    "responsive"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/alongite/alt-vue-grid-layout.git"
  },
  "author": "alongite",
  "main": "dist/alt-vue-grid-layout.umd.js",
  "unpkg": "dist/alt-vue-grid-layout.umd.min.js",
  "scripts": {
